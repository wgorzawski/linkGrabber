{
  "manifest_version": 3,
  "name": "Save URL to Clipboard",
  "version": "1.0",
  "description": "Save the current page's URL with the title and group them.",
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
